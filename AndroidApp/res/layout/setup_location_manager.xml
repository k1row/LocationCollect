<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <Button
        android:id="@+id/btnSetParameter"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
		android:onClick="onClick"
        android:text="setParameter" />

    <Button
        android:id="@+id/btnUpdateAraeInformation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
		android:onClick="onClick"
        android:text="UpdateAreaInformation" />

    <Button
        android:id="@+id/btnManualCheck"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
		android:onClick="onClick"
        android:text="手動で確認" />

    <Button
        android:id="@+id/btnLocationManagerCheck"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
		android:onClick="onClick"
        android:text="LocationManagerで確認" />

</LinearLayout>
